service fps_hal.oppo_realme_trinket /system/bin/hw/android.hardware.biometrics.fingerprint@2.1-service.oppo_realme_trinket
    # "class hal" causes a race condition on some devices due to files created
    # in /data. As a workaround, postpone startup until later in boot once
    # /data is mounted.
    class late_start
    user system
    group system input uhid
